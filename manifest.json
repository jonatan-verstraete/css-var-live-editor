{
  "manifest_version": 3,
  "name": "DoubleDash Editor",
  "description": "DevTools CSS variable editor for fast live overrides.",
  "version": "0.3.0",
  "permissions": ["clipboardWrite", "scripting", "storage"],
  "devtools_page": "devtools.html",
  "icons": {
    "16": "./assets/favicon-16x16.png",
    "32": "./assets/favicon-32x32.png",
    "192": "./assets/favicon-192x192.png",
    "512": "./assets/favicon-512x512.png"
  }
}
